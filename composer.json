{
  "name": "arrowsphere/entities",
  "type": "library",
  "description": "A PHP library to manage entities",
  "keywords": [
    "arrowsphere"
  ],
  "license": "MIT",
  "homepage": "https://www.arrow.com/ecs/na/solutions/cloud/arrowsphere/",
  "support": {
    "issues": "https://github.com/ArrowSphere/entities/issues",
    "source": "https://github.com/ArrowSphere/entities"
  },
  "authors": [
    {
      "name": "Pierre Feyssaguet",
      "email": "pfeyssaguet@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "ArrowSphere\\Entities\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArrowSphere\\Entities\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
